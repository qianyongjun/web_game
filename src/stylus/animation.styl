body,div,h1,h2,h3,h4,h5,h6,p,ul,li,dl,dt,dd,ol
	padding 0
	margin 0
ul,li,ol,dl,dt,dd
	list-style-type none
#content
	width 100%
	height 100%
	position absolute
	top 0
	left 0
	right 0
	bottom 0
	overflow hidden
	.content-wrap
		position relative
		>li
			float left
			overflow hidden
			position relative
			.a_background
				width 100%
				height 100%
				position absolute
				.bg_top
					width 100%
					height 71.6%
					background-image url('/images/background/a_background_top.png')
					background-repeat no-repeat
					background-size 100% 100%
				.bg_middle
					width 100%
					height 13.1%
					background-image url('/images/background/a_background_middle.png')
					background-repeat no-repeat
					background-size 100% 100%
				.bg_bottom
					width 100%
					height 15.3%
					background-image url('/images/background/a_background_bottom.png')
					background-repeat no-repeat
					background-size 100% 100%
			#sun
				width 201px
				height 201px
				position absolute
				top -30px
				right 40%
				z-index 1
				background url('/images/sun.png') 0 0 no-repeat
			.sunmove
				-webkit-animation sun-move 30s
				-moz-animation sun-move 30s
			#cloud
				.cloud
					position absolute
					z-index 2
				.cloud1
					background url('/images/cloud1.png') 0 0 no-repeat
					width 181px
					height 101px
					top 15%
					left -5%
				.cloud2
					background url('/images/cloud2.png') 0 0 no-repeat
					width 301px
					height 140px
					top 0
					right -5%
			.b_background_dark
				width 100%
				height 100%
				position absolute
				background-image url('/images/background/b-dark.png')
				background-size 100% 100%
			.b_background_bright
				width 100%
				height 100%
				position absolute
				background url('/images/background/b-bright.png')  no-repeat -9999px -9999px
			.b_lightup
				background url('/images/background/b-bright.png')
				background-size 100% 100%
			.shop
				position absolute
				width 39.5%
				height 35%
				left 29%
				top 36.5%
				.door
					position absolute
					width 32%
					height 68%
					top 33%
					left 58.5%
					overflow hidden
					z-index
					.door-left,.door-right
						position absolute
						width 50%
						height 100%
					.door-left
						background-image url('/images/door-left.png')
						background-size 100% 100%
						left 0
					.door-right
						background-image url('/images/door-right.png')
						background-size 100% 100%
						left 50%
			.bird
				position absolute
				background url('/images/bird.png') 0 0 no-repeat
				width 91px
				height 71px
				right -91px
				top 10%
				z-index 10
	.person
		width 151px
		height 291px
		position absolute
		left 0%
		top 55%
		background url('/images/boy.png') 0 -291px no-repeat
	.cloud1Move
		-webkit-animation smallCloud 30s infinite
		-moz-animation smallCloud 30s infinite
	.cloud2Move
		-webkit-animation largeCloud 40s infinite
		-moz-animation largeCloud 40s infinite
	.walk
		-webkit-animation-name person-slow
		-webkit-animation-duration 950ms
		-webkit-animation-iteration-count infinite
		-webkit-animation-timing-function steps(1, start)
		-moz-animation-name person-slow
		-moz-animation-duration 950ms
		-moz-animation-iteration-count infinite
		-moz-animation-timing-function steps(1, start)
	.pause
		-webkit-animation-play-state paused
		-moz-animation-play-state paused
	.flower
		-webkit-animation-name person-flower
		-webkit-animation-duration 950ms
		-webkit-animation-iteration-count infinite
		-webkit-animation-timing-function steps(1, start)
		-moz-animation-name person-flower
		-moz-animation-duration 950ms
		-moz-animation-iteration-count infinite
		-moz-animation-timing-function steps(1, start)
	.fly
		 -webkit-animation-name: birdFly;
		-webkit-animation-duration: 400ms;
		-webkit-animation-timing-function: step-start;
		-webkit-animation-iteration-count: infinite;
		-moz-animation-name: birdFly;
		-moz-animation-duration: 400ms;
		-moz-animation-timing-function: step-start;
		-moz-animation-iteration-count: infinite;
