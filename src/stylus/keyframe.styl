@-webkit-keyframes sun-move {
    0% {
        transform: translateX(0) translateY(0)
    }
    100% {
        transform: translateX(-2000px) translateY(400px)
    }
}

@-moz-keyframes sun-move {
    0% {
        transform: translateX(0) translateY(0)
    }
    100% {
        transform: translateX(-2000px) translateY(400px)
    }
}


@-webkit-keyframes smallCloud {
    0% {
        left: 5%
    }
    100% {
        left: 100%
    }
}

@-moz-keyframes smallCloud {
